{
    "version": 2,
    "builds": [
      { "src": "next.config.js", "use": "@vercel/next" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_SITE_NAME": "mlg-portfolio"
    },
    "rewrites": [],
    "redirects": [],
    "headers": [],
    "cleanUrls": true,
    "trailingSlash": false
  

    "env": {
      "NEXT_PUBLIC_DEPLOY_FROM": "dev-branch"
    }
}